@model IEnumerable<MVCStrongViewApp.Models.StudentModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index1</title>
</head>
<body>
    <table style="background-color:antiquewhite" border="1">
        <tr>
            <th>Student ID</th>
            <th>Student Name</th>
            <th>Photo</th>
        </tr>
        @foreach(var obj in Model)
            {
                <tr>
                    <td>@obj.StudID</td>
                    <td>@obj.StudName</td>
                    <td><img src="~/images/@obj.StudPhoto" height="70" width="70" /></td>
                </tr>
            }
    </table>
</body>
</html>
